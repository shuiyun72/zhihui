

.right>.link {
  line-height: 24px;
}

.table_msg_e_address {
  .pro, .next {
    text-align: center;
    font-size: 20px;
  }

  .item {
    padding: 7px 0;
  }

  padding: 10px 20px 10px 0;
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);
  background-color: #fff;
  width: 100px;
  box-sizing: border-box;

  .msg {
    color: #333;
  }

  .point {
    display: inline-block;
    width: 4px;
    height: 4px;
    background-color: #f00;
    border-radius: 50%;
  }

  color: #999;
  width: 1200px;
  margin: 0 auto;
}
