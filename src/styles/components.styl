.table_list_money{
  width: 100%;
  padding: 10px;
  padding-bottom: 0;
  font-size: 12px;
  box-sizing: border-box;
  .item{
    padding-bottom: 16px;
    :last-child{
      padding-bottom: 0;
    }
    .user{
      color: #01B0B7;
    }
    .content{
      width: 164px;
      display: inline-block;
      margin-right: 8px;
    }
    .red{
      color: #f00;
    }
  }
}
.self_help_box {
  width: 1200px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  padding-bottom: 20px;
}
.self_help {
  background: url("~@assets/img/houseBg.png");
  background-size: 290px auto;
  display: flex;
  justify-content: space-between;
  padding: 20px;
  img {
    width: 110px;
  }
  p {
    font-size: 20px;
    color: #666;
    padding: 30px 0 30px 10px;
  }
  button {
    background-color: #07b536;
    color: #fff;
    padding: 8px 24px;
    font-size: 16px;
    border-radius: 30px;
  }
}
.part_tab_e {
  width: 260px;
  display: flex;
  justify-content: space-between;
  font-size: 16px;
  .item {
    border-bottom: 1px solid transparent;
    align-self: flex-end;
    padding-bottom: 5px;
    &.is_active {
      border-bottom: 1px solid #f88f00;
      color: #f88f00;
    }
  }
}
.hui_box_bg {
  background-color: #f4f4f4;
}
.badge_num_box {
  width: 1200px;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  padding: 40px 0 20px;
  & > div {
    border-right: 1px solid #999;
    width: 100%;
    text-align: center;
    padding: 10px 0;
    &:last-child {
      border-right: none;
    }
    .badge_num {
      font-size: 30px;
      color: #666;
      position: relative;
      display: inline;
      span {
        font-size: 18px;
        position: absolute;
        right: -20px;
        top: 0px;
      }
    }
    p {
      font-size: 18px;
      text-align: center;
      color: #666;
      padding-top: 8px;
    }
  }
}
.blue_title {
  padding-top: 20px;
  text-align: center;
  color: #06a8f0;
  .t1 {
    font-size: 26px;
  }
  .t2 {
    font-size: 18px;
    padding: 16px 0 10px;
  }
}
.bg_img_top {
  width: 100%;
  height: 560px;
  background: url("~@assets/img/cimg.png") no-repeat center center;
  background-size: 1920px 560px;
}
.part_title {
  display: flex;
  justify-content: space-between;
  padding-bottom: 20px;
  align-items: flex-end;
  .text {
    font-size: 36px;
    color: #000;
  }
}
.share {
    align-items: flex-end;
    // font-size: 24px;
    i {
        // font-size: 26px;
        margin-left: 8px;
    }
    i:nth-child(2){
        color: #7cb6f1;
    }
    i:nth-child(3){
        color: #b0c635;
    }
    i:nth-child(4){
        color: #f7b9ec;
    } 
}
.new_box_item_sy {
  box-shadow: 0px 3px 6px rgba(0, 0, 0, 0.16);
  display: flex;
  justify-content: space-between;
  padding: 20px 10px 20px 6px;
  &:not(:last-child) {
    margin-bottom: 10px;
  }
  .left_img {
    display: inline-flex;
    flex-direction: column;
    justify-content: center;
    padding: 2px;
    img {
      width: 180px;
      height: 100px;
    }
  }
  .right_content {
    width: 575px;
    padding-right: 15px;
    .title {
      font-size: 16px;
      padding-bottom: 10px;
    }
    p {
      color: #666;
      padding-bottom: 16px;
      font-size: 12px;
      line-height: 18px;
    }
  }
}
.paihang_list_com_sy {
  padding: 2px 20px 20px;
  .title {
    font-size: 18px;
    padding: 8px 8px 8px 0;
    border-bottom: 1px solid #bbb;
  }
  p {
    font-size: 12px;
    padding: 12px 0 5px;
    &:nth-child(-n + 16):nth-child(n + 11) {
      span {
        padding: 0;
      }
    }
    &:nth-child(2) {
      span {
        background-color: #d60000;
        color: #fff;
        border: 1px solid #d60000;
      }
    }
    &:nth-child(3) {
      span {
        background-color: #db671f;
        color: #fff;
        border: 1px solid #db671f;
      }
    }
    &:nth-child(4) {
      span {
        background-color: #b1ad54;
        color: #fff;
        border: 1px solid #b1ad54;
      }
    }
    span {
      font-size: 12px;
      border: 1px solid #01b0b7;
      padding: 0px 4px 0 3px;
      display: inline-block;
      color: #01b0b7;
      margin-right: 6px;
    }
  }
}
.home2_titlle_show_sy {
  width: 1200px;
  margin: 0 auto;
  padding: 20px 0 10px;
  display: flex;
  justify-content: space-between;
  .part_title_e {
    font-size: 22px;
    border-bottom: 1px solid #999;
    padding-right :6px
    padding-bottom: 6px;
    color: #333;
    span:nth-child(2) {
      font-size: 14px;
      margin-left: 12px;
      color: #999;
    }
  }
}
.new_goods_com_sy{
  position:relative;
  color: #333;
  .state{
    position:absolute;
    top: 20px;
    left: 0;
    background-color: #00966E;
    color: #fff;
    padding: 6px 20px;
    border-radius: 0 20px 20px 0;
  }
  img{
    width: 100%;
    height: auto;
  }
  .box{
    padding: 10px 20px;
  }
  .title{
    font-size: 14px;
    margin-bottom: 6px;
    font-weight: bold;
  }
  p{
    font-size: 12px;
    margin-bottom: 6px;
  }
}

.new_head_title_sy{
  color: #333;
  border-bottom: 1px solid #333;
  display: inline-block;
  padding-bottom: 2px;
  margin-bottom: 10px;
  .title{
    font-size: 14px;
    font-weight: bold;
    margin-right: 4px;
  }
  span{
    font-size: 10px;
  }
}

.select_box_com_sy{
  font-size: 12px;
  box-sizing:border-box;
  padding: 16px 40px;
  color: #666;
  width: 100%;
  .item{
    display: flex;
    justify-content: space-between;
    margin-bottom: 10px;
    line-height: 14px;
    &:last-child{
      margin-bottom: 0;
    }
    .title{
      width: 60px;
      padding-top: 4px;
    }
    ul{
      display: inline-flex;
      justify-content: space-between;
      width:980px ;
      li{
        padding:4px 10px ;
        border-radius: 4px;
        &.is_active{
          color: #fff;
          background-color: #32BDC2;
        }
      }
    }
    .more{
      font-size: 12px;
      padding-top: 4px;
    }
  }
}
.link_more .el-icon-caret-bottom{
    font-size: 26px;
}